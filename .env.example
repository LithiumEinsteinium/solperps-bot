# Wallet
# Generate with: solana-keygen new -o wallet.json
# Then convert to JSON array format
WALLET_PRIVATE_KEY=[]

# RPC Endpoint
RPC_URL=https://api.mainnet-beta.solana.com

# Telegram Bot (optional)
# Get from @BotFather on Telegram
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=

# Bot Configuration
PAPER_TRADING=true
AUTO_TRADE=false

# Trading Settings
POSITION_SIZE=10
LEVERAGE=1
SYMBOL=SOL

# Strategy: ma-cross, rsi, signal
STRATEGY=ma-cross
AUTO_EXECUTE=false

# Signal file path
SIGNAL_FILE=./data/signal.json

# Position storage
POSITION_STORAGE=./data/positions.json
